{
    "id": "experiment",
    "model": 
    {
        "name": "cSA-BLSTM",
        "config":
        {
            "max_epoch": 100,
            "batch_size": 64,
            "embed_dim": 256,
            "output_dim": 512,
            "num_layers": 3,
            "hidden_size": 512
        }
    },

    "optimizer":
    {
        "name": "Adam",
        "config":
        {
            "lr": 3e-4,
            "betas": [0.9, 0.999],
            "epsilon": 1e-8
        }
    },

    "loss_function": "CSALoss"
}
